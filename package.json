{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:cloudflare": "turbo run build --filter=@mui-theme-builder/theme-library && pnpm --filter @repo/theme-studio build:cloudflare",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start": "pnpm --filter @repo/theme-studio start",
    "prod": "pnpm build && pnpm start",
    "preview": "pnpm --filter @repo/theme-studio preview",
    "deploy": "pnpm --filter @repo/theme-studio deploy"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^2.6.0"
  },
  "packageManager": "pnpm@8.15.6",
  "name": "with-vite"
}
